{
  "name": "@hopdrive/lambda-timeout-wrapper",
  "version": "1.0.1",
  "description": "A utility to handle AWS Lambda timeouts gracefully",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "deploy": "node scripts/deploy-wrapper.js",
    "deploy-force": "node scripts/modified-deploy.js"
  },
  "keywords": [
    "aws",
    "lambda",
    "timeout",
    "wrapper",
    "utility"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@hopdrive/package-deploy-scripts": "0.0.4",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "rimraf": "^4.4.1",
    "eslint": "^8.37.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
